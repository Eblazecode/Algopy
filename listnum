# ARRAY NUMBERS
numbers = [1,3,4,5,6,1]
for x in numbers:
    print(x)

# exponentiating each output
for x in numbers:
    x **=x
    print(x)

# program to find perfect square from 1 - 100
squares = [1,2,3,4,5,6,7,8,9]
print("perfect squares 1-100\n")
for x in squares:
    x **=2
    print(x)

number = 3
for y in range(1,10):
    print(y,"*",number,"=" ,y * number)

